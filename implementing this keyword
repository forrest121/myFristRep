class thisDemo{
    int a,b;
    public void get(int a , int b){
        this.a = a;
        this.b = b;
    }
    public void put(){
        System.out.println("a="+a);
        System.out.println("b="+b);
    }
}
class Main {
   public static void main(String[]args){
       thisDemo obj1 = new thisDemo();
       obj1.get(10,20);
       obj1.put();
   }
}
